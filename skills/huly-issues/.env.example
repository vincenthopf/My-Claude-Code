# Huly Server Configuration
# Copy this file to .env and fill in your values

# Base URL for your Huly instance (v7 uses nginx proxy)
HULY_URL=http://localhost:8087

# Authentication credentials
HULY_EMAIL=user@example.com
HULY_PASSWORD=your-password-here
HULY_WORKSPACE=your-workspace-name

# Optional: JWT Token (the client handles auth automatically)
HULY_TOKEN=
